/* pages/prize/takelist.wxss */
.money-wrap{
  margin:0 auto;
  padding:20rpx 2.5%;
  color:#e9e9e9;
}
.money-item{
  margin:0 2%;
}
.money-text{
  
  font-size:32rpx;
  text-align:center;
  line-height:30rpx;
}
.show-money{
  font-size:40rpx;
  line-height:50rpx;
  margin:0 10rpx;
}

/* 列表区域 */
.list-wrap{
  
}
.list-wrap:last-child{
  margin-bottom:0;
}
.list-title{
  padding:30rpx 2.5% 30rpx 80rpx;
  position:relative;
  height:110rpx;
  line-height:50rpx;
}
.title-img{
  width:55rpx;
  position:absolute;
  margin:auto;
  top:0rpx;
  bottom:0;
  left:2.5%;
}
.item-title{
  float:left;
  font-size: 30rpx;
  
}
.item-time{
  float:right;
  color:#666;
}
